<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
<bean id="Question" class ="Question2.Question">
	<property name="questionId" value="123" />
	<property name="ques" value="What is Java?" />
	<property name="answers">
		<list>
			<value >"Java is OOP."</value>
			<value >"Java is OOP1."</value>
		</list>
	</property>
	
	<property name="ansSet">
		<set>
			<value >"Java is OOP."</value>
			<value >"Java is OOP1."</value>
		</set>
	</property>
	
	<property name="ansMap">
		<map>
			<entry key="1">
				<value> "Java is OOPs "</value>
			</entry>
			
			<entry key="2">
				<value>"Java is OOP2"</value>
			</entry>
		</map>
	</property>
	
</bean>


</beans>